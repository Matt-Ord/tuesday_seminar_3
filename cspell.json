{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
    "version": "0.2",
    "language": "en",
    "words": [
        "noqa",
        "lindblad",
        "Feijoa",
        "Langevin",
        "Caldeira",
        "Frolich"
    ],
    "ignoreWords": [
        "Phonopy",
        "Wannier",
        "pytest",
        "pydata",
        "pylint",
        "pydocstyle",
        "numpy",
        "isort",
        "ndarray",
        "xlabel",
        "ylabel",
        "pyproject",
        "scipy",
        "trange",
        "tqdm",
        "sympy",
        "ipython",
        "nbconvert",
        "nbformat",
        "pygments",
        "pyplot",
        "imag",
        "axline",
        "xytext",
        "ipykernel",
        "lambdify",
        "tspan",
        "unpickler",
        "savez",
        "pickler",
        "strat",
        "sdeint",
        "evalf",
        "cbar",
        "ylim",
        "xlim",
        "xscale",
        "yscale",
        "pcolormesh",
        "dtype",
        "ufuncs",
        "xaxis",
        "yaxis",
        "nbins",
        "axhline",
        "twinx",
        "nrows",
        "as_numer_denom",
        "bbox",
        "ncols",
        "blit",
        "atanh",
        "isinstance",
        "basedpyright",
        "mathrm",
        "Fock",
        "annot"
    ],
    "flagWords": [
        "hte",
        "teh"
    ],
    "allowCompoundWords": true,
    "maxNumberOfProblems": 1000,
    "ignorePaths": [
        "*.snap",
        "node_modules",
        "__snapshots__",
        "package-lock.json",
        "coverage/**",
        "dist/**",
        "dist.*/**",
        "temp/**",
        "CHANGELOG.md",
        "package.json",
        ".cspell.json",
        "cspell.json",
        ".vscode/**",
        ".devcontainer/**",
        ".github/**",
        ".pytest_cache/**",
        ".ruff_cache/**",
        ".venv/**",
        ".vscode/**",
        "build/**",
        "docs/**",
        "*.egg-info/**",
        ".gitignore",
        "*.svg",
    ],
    "noConfigSearch": true
}